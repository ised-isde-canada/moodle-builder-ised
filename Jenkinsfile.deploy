@Library(value="ised-cicd-lib") _

pipeline {
	agent none

    options {
        disableConcurrentBuilds()
    }

   	environment {
		// GLobal Vars
		IMAGE_NAME = "dsdmdl-moodle"
    }

    stages {
    	stage('deploy') {
			steps {
				script {
    				deploy("${IMAGE_NAME}")
    			}
    		}
    	}
    }
}